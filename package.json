{
  "name": "merge-table-cells",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/merge-table-cells.umd.cjs",
  "module": "./dist/merge-table-cells.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/merge-table-cells.js",
    "require": "./dist/merge-table-cells.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "concurrently \"npm run start:angular\" \"npm run start:vue2\" \"npm run start:vue3\" \"npm run dev\"",
    "start:vue3": "cd examples/vue3-elementplus && npm run dev",
    "start:vue2": "cd examples/vue2-elementui && npm run serve",
    "start:angular": "cd examples/angular-tinyui && npm run start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}